ransition: 0.4s ease;\r\n  }\r\n  \r\n  .nav-link:hover {\r\n    color: $primary-color;\r\n  }\r\n  \r\n  .hamburger {\r\n    display: none;\r\n    cursor: pointer;\r\n    float: right;\r\n    margin-top: -110px;\r\n  }\r\n  \r\n  .bar {\r\n    display: block;\r\n    width: 25px;\r\n    height: 3px;\r\n    margin: 5px auto;\r\n    -webkit-transition: all 0.3s ease-in-out;\r\n    transition: all 0.3s ease-in-out;\r\n    background-color: $black-color;\r\n  }\r\n  \r\n  @media (max-width: $size-large) {\r\n\r\n    .hamburger {\r\n      display: block;\r\n    }\r\n  \r\n    .hamburger.active .bar:nth-child(2) {\r\n      opacity: 0;\r\n    }\r\n\r\n    .hamburger.active .bar:nth-child(1) {\r\n      transform: translateY(8px) rotate(45deg);\r\n    }\r\n\r\n    .hamburger.active .bar:nth-child(3) {\r\n      transform: translateY(-8px) rotate(-45deg);\r\n    }\r\n  \r\n    .nav-menu {\r\n      position: fixed;\r\n      left: -200%;\r\n      top: 360px;\r\n      gap: 0;\r\n      flex-direction: column;\r\n      width: 100%;\r\n      text-align: center;\r\n      transition: 0.3s;\r\n\r\n      @media (min-width: $size-small) {\r\n        top: 200px;\r\n      }\r\n\r\n      @media (min-width: $size-medium) {\r\n        top: 200px;\r\n      }\r\n    }\r\n\r\n    .nav-item {\r\n      margin: 16px 0;\r\n    }\r\n  \r\n    .nav-menu.active {\r\n      left: 0;\r\n    }\r\n  }","css\\partials\\_colors.scss":"$primary-color: #ecc7d0;\r\n$black-color: #000000;\r\n$details-color: #a3a2a2;\r\n$white-color: #FFFF;\r\n\r\n","css\\shopping.scss":"@use 'partials/_colors.scss' as *;\r\n@use 'partials/breakpoints' as *;\r\n\r\n.shopping {\r\n    display: flex;\r\n    flex-wrap: wrap;\r\n    display: flex;\r\n    align-items: center;\r\n  \r\n    @media (min-width: $size-large) {\r\n      flex-direction: row;\r\n      justify-content: space-between;\r\n    }\r\n  \r\n    &_form {\r\n      width: 380px;\r\n      height: 40px;\r\n      display: flex;\r\n      justify-content: space-between;\r\n      align-items: center;\r\n      border-bottom: 1px solid $black-color;\r\n\r\n      @media(min-width: $size-small){\r\n        margin-left: auto;\r\n      }\r\n\r\n      @media(min-width: $size-large){\r\n        margin-left: 0;\r\n      }\r\n\r\n    }\r\n  \r\n    &_input {\r\n      border: none;\r\n      padding-left: 10px;\r\n  \r\n      &:focus {\r\n        outline: none;\r\n      }\r\n    }\r\n  \r\n    &_img {\r\n      border: none;\r\n      cursor: pointer;\r\n      background-color: $white-color;\r\n      padding: 10px 6px;\r\n    }\r\n  }","css\\main.scss":"@use 'partials/_colors.scss' as *;\r\n@use 'partials/_fonts.scss' as *;\r\n@use 'partials/_breakpoints' as *;\r\n@use 'src/css/hamburger.scss' as *;\r\n@use 'src/css/shopping.scss' as *;\r\n\r\nbody {\r\n  margin: 0;\r\n  background-color: $white-color;\r\n}\r\n\r\na {\r\n  color: $black-color;\r\n  text-decoration: none;\r\n}\r\n\r\n.header {\r\n  display: flex;\r\n  justify-content: space-between;\r\n  white-space: nowrap;\r\n  flex-wrap: wrap;\r\n\r\n  &_left {\r\n    display: flex;\r\n    gap: 20px;\r\n    padding-top: 9px;\r\n  }\r\n\r\n  &_right {\r\n    display: flex;\r\n  }\r\n\r\n  &_image {\r\n    padding-right: 10px;\r\n  }\r\n}\r\n\r\n.categories {\r\n  text-transform: uppercase;\r\n\r\n  ul {\r\n    li {\r\n      list-style-type: none;\r\n      float: left;\r\n    }\r\n  }\r\n}\r\n\r\n.header-button {\r\n  cursor: pointer;\r\n  background-color: $white-color;\r\n  padding: 10px 20px;\r\n  border-width: 1px;\r\n  border-bottom: none;\r\n  border-color: $details-color;\r\n\r\n  @media(min-width: $size-small){\r\n    border-top: none;\r\n  }\r\n\r\n  &_account {\r\n    border-left: none;\r\n    border-color: $details-color;\r\n  }\r\n\r\n  &_basket {\r\n    cursor: pointer;\r\n    padding: 36px 20px;\r\n    display: flex;\r\n  }\r\n}\r\n\r\nhr {\r\n  width: 100%;\r\n  height: 1px;\r\n  background-color: $details-color;\r\n  border-top: 1px;\r\n  border-bottom: 0;\r\n}\r\n\r\n* {\r\n  padding: 0;\r\n  margin: 0;\r\n  box-sizing: border-box;\r\n}\r\n\r\n.container {\r\n  max-width: 300px;\r\n  margin: 30px auto;\r\n\r\n  &_top {\r\n    max-width: 300px;\r\n    margin: 0 auto;\r\n  }\r\n\r\n  .nav-menu {\r\n    padding: 0 70px;\r\n  }\r\n\r\n  @media (min-width: $size-small) {\r\n    max-width: 550px;\r\n    margin: 30px auto;\r\n\r\n    &_top {\r\n      max-width: 550px;\r\n      margin: 0 auto;\r\n    }\r\n  }\r\n\r\n  @media (min-width: $size-large) {\r\n    max-width: 1190px;\r\n    margin: 30px auto;\r\n\r\n    &_top {\r\n      max-width: 1190px;\r\n      margin: 0 auto;\r\n    }\r\n  }\r\n}\r\n\r\n.line {\r\n  background-color: $white-color;\r\n\r\n  @media (min-width: $size-large) {\r\n    background-color: $details-color;\r\n  }\r\n\r\n  &-top {\r\n    background-color: $details-color;\r\n  }\r\n}\r\n"},"lineCount":null}},"error":null,"hash":"2d6977291969273643848260580b51e4","cacheData":{"env":{}}}